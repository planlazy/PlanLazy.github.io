<html>
	<head>
		<title><%= title %> - PlanLazy</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="/assets/travel-photos/js/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="/assets/travel-photos/css/main.css" />
		<!--[if lte IE 9]><link rel="stylesheet" href="/assets/travel-photos/css/ie9.css" /><![endif]-->
		<!--[if lte IE 8]><link rel="stylesheet" href="/assets/travel-photos/css/ie8.css" /><![endif]-->
		<noscript><link rel="stylesheet" href="/assets/travel-photos/css/noscript.css" /></noscript>
		<link type="text/plain" rel="author" href="http://PlanLazy.com/humans.txt" />
		<meta name="p:domain_verify" content="dd2f670c287fc95ca2b98a5a47f599b1"/><!-- Pintrest pins our logo -->
		<meta property="og:site_name" content="PlanLazy"/>
		<meta property="og:url" content="http://planlazy.com/travel-photos/<%= urn %>/" />
		<meta property="og:title" content="<%= title %>" />
		<meta property="og:description" content="<%= description %>" />
		<% if (image) { %><meta property="og:image" content="http://planlazy.com/travel-photos/<%= urn %>/<%= urn %>.jpeg" />
		<meta property="og:image:url" content="http://planlazy.com/travel-photos/<%= urn %>/<%= urn %>.jpeg" />
		<meta property="og:image:type" content="image/jpeg" /> 
		<meta property="og:image:width" content="<%= image.width %>" />
		<meta property="og:image:height" content="<%= image.height %>" />
		<meta name="twitter:card" content="summary_large_image">
		<meta name="twitter:site" content="@PlanLazy">
		<meta name="twitter:creator" content="@PlanLazy">
		<meta name="twitter:title" content="<%= title %>">
		<meta name="twitter:description" content="<%= description %>">
		<meta name="twitter:image" content="http://planlazy.com/travel-photos/images/<%= urn %>.jpeg"><% } %><% if (youtube) { %><meta property="og:image" content="https://i.ytimg.com/vi/<%= youtube.id %>/maxresdefault.jpg">
		<meta property="og:image:secure_url" content="https://i.ytimg.com/vi/<%= youtube.id %>/maxresdefault.jpg" />
		<meta property="og:video" content="https://www.youtube.com/embed/<%= youtube.id %>" />
		<meta property="og:video:secure_url" content="https://www.youtube.com/embed/<%= youtube.id %>" />
		<meta name="twitter:card" content="summary_large_image">
		<meta name="twitter:site" content="@PlanLazy">
		<meta name="twitter:title" content="<%= title %>">
		<meta name="twitter:description" content="<%= description %>">
		<meta name="twitter:image" content="https://i.ytimg.com/vi/<%= youtube.id %>/maxresdefault.jpg"><% } %>
		<meta property="fb:app_id" content="1481567445473126" />
	</head>
	<body class="is-loading">

		<!-- Wrapper -->
			<div id="wrapper">

				<!-- Main -->
					<section id="main">
						<a href="/"><img src="/assets/travel-photos/images/logo-medium-600x72.png" id="logo"></a>
						<h1><%= subject %></h1>
						<h2><%= location %></h2>
						<% if (image) { %>
							<div id="photo" class="<%= image.shape %>" style="background-image: url('/travel-photos/<%= urn %>/<%= urn %>.jpeg');"><%= description %></div>
						<% } %>
						<% if (youtube) { %>
							<iframe id="video" src="https://www.youtube.com/embed/<%= youtube.id %>" frameborder="0" allowfullscreen></iframe>
						<% } %>
						<p><%= description %></p>
					</section>

				<!-- Footer -->
					<footer id="footer">
						<ul class="copyright">
							<li>Design: <a href="http://html5up.net/">HTML5 UP</a></li>
						</ul>
					</footer>

			</div>

		<!-- Scripts -->
			<!--[if lte IE 8]><script src="/assets/travel-photos/js/respond.min.js"></script><![endif]-->
			<script>
				if ('addEventListener' in window) {
					window.addEventListener('load', function() { document.body.className = document.body.className.replace(/\bis-loading\b/, ''); });
					document.body.className += (navigator.userAgent.match(/(MSIE|rv:11\.0)/) ? ' is-ie' : '');
				}
			</script>
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			ga('create', 'UA-66724222-1', 'auto');
			ga('require', 'linkid', 'linkid.js');
			ga('send', 'pageview');
		</script>
	</body>
</html>